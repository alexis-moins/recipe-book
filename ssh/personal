# vim: ft=sshconfig
Host *
    AddKeysToAgent yes
    UseKeychain yes

Host gigz
    Hostname 82.126.253.221
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/gigz

    Port 777
    User alexis

Host github.com
    Hostname github.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/github

Host github-unifox
    Hostname github.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/unifox
